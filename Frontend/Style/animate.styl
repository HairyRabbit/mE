/**
 * Init Animate.
 */

 

trans(delay = 0s)
  transition all 0.6s ease-in-out delay
  

.anim

  // justme.
  
  .justme
    opacity 1
    trans(0.2s)

    
  // nav.
    
  .nav-item
    transform translate3d(0, 0, 0)
    opacity 1

    &:nth-child(1)
      trans()

    &:nth-child(2)
      trans(0.2s)

    &:nth-child(3)
      trans(0.4s)

      
  // logo.
  
  .logo
    transform translate3d(0, 0, 0)
    opacity 1
    trans()
